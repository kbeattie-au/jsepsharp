<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0; netstandard2.1</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>JsepSharp</Title>
    <Description>A .NET implementation of the Jsep JavaScript Expression Parser. Designed to parse value expressions, like those written in individual spreadsheet cells, and not operations.</Description>
    <RepositoryType>git</RepositoryType>
	<DebugType>portable</DebugType>
    <NeutralLanguage>en-US</NeutralLanguage>
	<LangVersion>12.0</LangVersion>
	<AssemblyVersion>1.3.8.0</AssemblyVersion>
	<FileVersion>1.3.8.0</FileVersion>
	<EnableNETAnalyzers>true</EnableNETAnalyzers>
	<AnalysisLevel>latest</AnalysisLevel>
	<EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <IsAotCompatible>False</IsAotCompatible>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <IsAotCompatible>False</IsAotCompatible>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
</Project>
